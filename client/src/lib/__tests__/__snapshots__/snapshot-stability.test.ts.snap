        except Exception as btn_error:
            logging.warning(f"–ù–µ —É–¥–∞–ª–æ—Å—å –∏–∑–≤–ª–µ—á—å –∫–Ω–æ–ø–∫–∏: {btn_error}")

    # –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–∏–Ω—Ö—Ä–æ–Ω–Ω–æ –¥–ª—è –≥–∞—Ä–∞–Ω—Ç–∏–∏ –¥–æ—Å—Ç–∞–≤–∫–∏
    await save_message_to_api(
        user_id=str(chat_id),
        message_type="bot",
        message_text=text,
        node_id=node_id,
        message_data=message_data_obj
    )
    return result

