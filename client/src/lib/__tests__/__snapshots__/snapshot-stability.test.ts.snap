        user_id = str(event.from_user.id)
        message_text = event.text or event.caption or "[–º–µ–¥–∏–∞]"
        message_data = {"message_id": event.message_id}

        # –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ —Ñ–æ—Ç–æ
