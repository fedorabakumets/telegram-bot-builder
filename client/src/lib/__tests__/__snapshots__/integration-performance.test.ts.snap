selected_options = {}

@dp.callback_query(lambda c: c.data.startswith("multi_select_"))
async def handle_multi_select_callback(callback_query: types.CallbackQuery):
    await callback_query.answer()
    
    callback_data = callback_query.data
    user_id = callback_query.from_user.id
    
    if callback_data.startswith("multi_select_done_"):
        # –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–Ω–æ–ø–∫–∏ "–ì–æ—Ç–æ–≤–æ" (continue_button)
        short_node_id = callback_data.replace("multi_select_done_", "")
        
        # –ù–∞—Ö–æ–¥–∏–º –ø–æ–ª–Ω—ã–π node_id –ø–æ –∫–æ—Ä–æ—Ç–∫–æ–º—É —Å—É—Ñ—Ñ–∏–∫—Å—É
        node_id = None
        if short_node_id == "rvey_start":
            node_id = "survey_start"
        
        if node_id:
            # –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É —É–∑–ª—É
            if node_id == "survey_start":
                await handle_callback_survey_result(callback_query)
    else:
        # –û–±—Ä–∞–±–æ—Ç–∫–∞ –≤—ã–±–æ—Ä–∞ —ç–ª–µ–º–µ–Ω—Ç–∞
        # –õ–æ–≥–∏–∫–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è/—É–¥–∞–ª–µ–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–∞ –∏–∑ –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö
        if user_id not in selected_options:
            selected_options[user_id] = []
        
        # –î–æ–±–∞–≤–ª—è–µ–º –∏–ª–∏ —É–¥–∞–ª—è–µ–º –≤—ã–±—Ä–∞–Ω–Ω—ã–π —ç–ª–µ–º–µ–Ω—Ç
        option_value = callback_data.replace("multi_select_", "")
        if option_value in selected_options[user_id]:
            selected_options[user_id].remove(option_value)
        else:
            selected_options[user_id].append(option_value)

