@dp.message(Command("help"))
async def help_handler(message: types.Message):
    logging.info(f"–ö–æ–º–∞–Ω–¥–∞ help –≤—ã–∑–≤–∞–Ω–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–º {message.from_user.id}")
