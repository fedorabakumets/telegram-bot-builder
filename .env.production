# Production Environment Variables
NODE_ENV=production

# Database (будет переопределено через переменные окружения платформы)
DATABASE_URL=${DATABASE_URL}

# Session Secret (будет переопределено через переменные окружения платформы)
SESSION_SECRET=${SESSION_SECRET}

# Server Configuration
PORT=${PORT:-8080}
HOST=0.0.0.0

# Logging
LOG_LEVEL=error

# Security
TRUST_PROXY=true