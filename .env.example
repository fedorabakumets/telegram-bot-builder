# База данных PostgreSQL
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/telegram_bot_builder
PGHOST=localhost
PGPORT=5432
PGDATABASE=telegram_bot_builder
PGUSER=postgres
PGPASSWORD=postgres

# Секретный ключ для сессий (замените на случайную строку)
SESSION_SECRET=your-super-secret-session-key-here-change-this-in-production

# Telegram API credentials (получите на https://my.telegram.org)
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here

# Режим работы
NODE_ENV=development